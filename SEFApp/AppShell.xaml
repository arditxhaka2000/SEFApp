<?xml version="1.0" encoding="UTF-8" ?>
<Shell x:Class="SEFApp.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:SEFApp.Views"
       Title="SEF Manager"
       >

    <!-- Login Page -->
    <ShellContent Title="Login" x:Name="LoginShellContent"
                  Route="LoginPage"
                  ContentTemplate="{DataTemplate local:LoginPage}" />

    <!-- Dashboard Page --><!--
    <ShellContent Title="Dashboard" 
                  x:Name="DashboardShellContent"
                  Route="DashboardPage"
                  ContentTemplate="{DataTemplate local:DashboardPage}" />-->

</Shell>